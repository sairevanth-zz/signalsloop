{
  "crons": [
    {
      "path": "/api/cron/orchestrator?schedule=morning",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/orchestrator?schedule=evening",
      "schedule": "0 21 * * *"
    },
    {
      "path": "/api/cron/scheduled-queries",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/generate-suggestions",
      "schedule": "0 6 * * *"
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/embed/:path*",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/((?!embed).*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
